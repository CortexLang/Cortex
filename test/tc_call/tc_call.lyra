// Fractal call-test
object tc_call::
	// AnimalKind
	enum AnimalKind ::
		constant MAMMAL
		constant REPTILE
		constant INSECT	
		constant FISH

	// Animal
	class Animal ::
		member kind : AnimalKind
		AnimalKind getKind()
		virtual bool matesWith(Animal m)
		
	// MammalKind
	enum MammalKind ::
		constant PRIMATE
		constant RODENTIA
		constant CHIROPTERA
		constant CARNIVORA
		
	// HabitatKind
	enum HabitatKind ::
		constant LAND
		constant WATER
		constant AIR
		
	// MammalProps (used to test functions with complex parameters)
	struct MammalProps ::
		member legs : uint32
		member habitat : HabitatKind
		member speed : float64

	// Mammal
	class Mammal : baseAccess:LOCAL base:Animal ::
		member kind : MammalKind
		member props : MammalProps, LOCAL
		MammalKind getKind() // Doesn't override Animal::getKind
		virtual bool matesWith(Animal m) // Overrides Animal::matesWith
		void setProps(MammalProps props)
		MammalProps getProps(uint32 someValue)
		void setSpeed(float64 speed)
		float64 getSpeed()
		type::init -> int16 init(Mammal object)
