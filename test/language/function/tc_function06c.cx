struct Point:: x, y: int32

void func(Point p, Point q):
    if p.x != 10: fail("p.x != 10")
    if p.y != 20: fail("p.y != 20")
    if "$p" != "{10,20}": fail("\"$p\" != \"{10,20}\"")
    if q.x != 30: fail("q.x != 30")
    if q.y != 40: fail("q.y != 40")
    if "$q" != "{30,40}": fail("\"$p\" != \"{30,40}\"")

Point a1: 10, 20
var Point b1 = {10, 20}
var Point& c1 = a1
var Point& d1 = Point{10, 20}

Point a2: 30, 40
var Point b2 = {30, 40}
var Point& c2 = a2
var Point& d2 = Point{30, 40}

func({10, 20}, {30, 40})
func(a1, a2)
func(b1, b2)
func(c1, c2)
func(d1, d2)

