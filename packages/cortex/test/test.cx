#package ::cortex::test

procedure test: base=method, kind=procedureKind::METHOD

struct TestResult::
    member success: bool, readonly
    member assertmsg: string
    member errmsg: string

class TestCase::
    member test: ::cortex::test::test
    member result: TestResult

    int16 construct()

    void setUp() virtual
    void tearDown() virtual

    void assert(bool b)
    void assert(bool b, string errmsg)
    void assertEqual(any a, any b)
    void assertEqual(any a, any b, string errmsg)
